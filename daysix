// criar variaveis no JS
`crase` para transformar em texto

// Criando condição no JS
condicao=confirm("Tem condição médica?")
if(condicao){condicaoresp="Sim"}
if(condicao==false){condicaoresp="Não"}
lista.innerHTML=
`<li> Condição pré-existente? ${condicaoresp}</li>`

